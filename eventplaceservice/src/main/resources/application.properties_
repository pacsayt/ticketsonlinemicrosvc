spring.application.name=eventplaceservice
management.security.enabled=false
logging.level.com.self.sprintboot.learning.employee=DEBUG

# HHH000205: Loaded properties from resource hibernate.properties:
spring.jpa.database-platform=org.hibernate.dialect.H2Dialect
spring.datasource.driverClassName=org.h2.Driver
spring.datasource.url=jdbc:h2:mem:eventplacedb;DB_CLOSE_DELAY=-1;DATABASE_TO_UPPER=false

spring.datasource.username=sa
spring.datasource.password=password

spring.jpa.generate-ddl=true
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=create-drop
spring.h2.console.enabled=true
spring.h2.console.path=/h2-console

spring.jpa.hibernate.naming-strategy=org.hibernate.cfg.ImprovedNamingStrategy

eureka.instance.leaseRenewalIntervalInSeconds=5
eureka.instance.leaseExpirationDurationInSeconds=2
eureka.client.serviceUrl.defaultZone=http://localhost:8761/eureka/
eureka.client.healthcheck.enabled=true
eureka.client.lease.duration=5
